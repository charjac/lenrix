{
  "name": "lenrix",
  "version": "0.4.5",
  "description": "Type-safe, reactive, lens-focused, immutable state management",
  "keywords": [
    "typescript",
    "rxjs",
    "lens",
    "immutable",
    "state"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/couzic/lenrix.git"
  },
  "homepage": "https://github.com/couzic/lenrix#readme",
  "bugs": {
    "url": "https://github.com/couzic/lenrix/issues"
  },
  "author": {
    "name": "Mikael Couzic",
    "email": "mikaelcouzic@gmail.com",
    "url": "https://github.com/couzic"
  },
  "scripts": {
    "prebuild": "npm run test && shx rm -rf ./lib",
    "build": "tsc",
    "prepare": "npm run build",
    "doctoc": "doctoc . --notitle --maxlevel 4",
    "test": "mocha --opts mocha.opts && npm run test-compilation-errors",
    "test-compilation-errors": "node ./testCompilationErrors.js",
    "tdd": "mocha --opts mocha.opts --watch",
    "lint": "tslint --project ./ --fix"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "peerDependencies": {
    "immutable-lens": "^0.3.0",
    "redux": "^4.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "doctoc": "^1.3.1",
    "glob-promise": "^3.4.0",
    "immutable-lens": "^0.3.3",
    "mocha": "^5.2.0",
    "prettier": "^1.13.4",
    "ramda": "^0.25.0",
    "redux": "^4.0.0",
    "rxjs": "^6.2.0",
    "rxjs-tslint": "^0.1.4",
    "shx": "^0.2.2",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1"
  }
}
